#!/usr/bin/env lua

require("sncl.sncl")

local argparse = require("argparse")

local argParser = argparse() {
	name = "sncl",
	description = "A sNCl compiler to NCL",
}
argParser:argument("input", "Input File"):args(1)

local args = argParser:parse()

arquivoEntrada = args.input
print(arquivoEntrada)
beginParse(arquivoEntrada)
