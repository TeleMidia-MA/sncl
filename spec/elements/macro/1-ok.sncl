macro m1(prop1, prop2, right)
   left: prop1
   top: prop2
   right: right
end

macro m2(prop)
   coords: prop
end

media media1 --media com uma area dentro
   *m1("50%", "60%", "70%")
   *m1("51%", "61%", "71%")
   area area1
      *m2("50%, 70%, 80%")
   end
end

context context1 --contexto
   *m1("50%", "60%", "70%")
end

region region1 --region
   *m1("50%", "60%", "70%")
end

